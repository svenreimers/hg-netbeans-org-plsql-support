       Fnd_Boolean_API.Decode(modify_row) modify_row,
       modify_row                     modify_row_db,
       Fnd_Boolean_API.Decode(remove_row) remove_row,
       remove_row                     remove_row_db,
       &OBJID                         objid,
       &OBJVERSION                    objversion
