<project name="org.netbeans.libs.commons" default="netbeans" basedir=".">
    <description>Builds, tests, and runs the project org.netbeans.libs.commons.</description>
    <import file="nbproject/build-impl.xml"/>
    <target name="files-init" depends="external,projectized-common.files-init"/>
    <target name="external">
        <echo message="Copying commons-io-1.4.jar from IDE"/>
        <mkdir dir="release/modules/ext"/>
        <copy file="${nbplatform.default.netbeans.dest.dir}/ide/modules/ext/commons-io-1.4.jar" todir="release/modules/ext/"/>
    </target>
    <target name="clean" depends="projectized-common.clean">
        <echo message="Cleaning release directory"/>
        <delete dir="release" failonerror="false"/>
    </target>
</project>
